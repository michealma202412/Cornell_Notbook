{
  "output": "matrix_notebook.pdf",
  "page_format": {
    "size": "A4",
    "orientation": "portrait",
    "margin": 20
  },
  "matrix": {
    "rows": 10,
    "cols": 6,
    "cell_defaults": {
      "border_enabled": true,
      "border_style": "solid",
      "border_color": "black",
      "border_sides": {
        "top": true,
        "bottom": true,
        "left": true,
        "right": true
      },
      "grid_type": "single_line",
      "grid_line_step_mm": 9,
      "fill_text": "",
      "text_alignment": "center",
      "vertical_alignment": "middle",
      "font_size": 10
    },
    "cells": [
      {
        "row": 0,
        "col": 0,
